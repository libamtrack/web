{
  "visibleName": "Proton depth-dose curve",
  "functionName": "AT_dose_Bortfeld_Gy",
  "description": "Analytical model of proton depth-dose curve (Bragg curve) according to Bortfeld model (DOI 10.1118/1.598116). The model assumes a pencil-beam like source, includes range straggling and initial kinetic energy spread.",
  "xTitle": "Depth [cm]",
  "yTitle": "Dose [Gy]",
  "plot": true,
  "formItems": [
    {
      "type": "entry_module",
      "parameterName": "z_cm",
      "label": "Depth [cm]",
      "startholder": "0.0",
      "endholder": "20.0",
      "intervalType": "points",
      "stepDefaultValue": "0.1",
      "pointsDefaultNumber": "500",
      "validations": {
        "type": "float",
        "min": "0.0",
        "max": "1000000000.0"
      }
    },
    {
      "type": "input",
      "parameterName": "E_MeV_u",
      "label": "Energy [MeV]",
      "placeholder": "150.0",
      "defaultValue": "150.0",
      "validations": {
        "type": "float",
        "min": "0.000001",
        "max": "1000000.0"
      }
    },
    {
      "type": "input",
      "parameterName": "sigma_E_MeV_u",
      "label": "Energy spread (Ïƒ) [MeV]",
      "placeholder": "1.5",
      "defaultValue": "1.5",
      "validations": {
        "type": "float",
        "min": "0.0",
        "max": "1000000.0"
      }
    },
    {
      "type": "input",
      "parameterName": "entrance_dose_Gy",
      "label": "Entrance dose [Gy]",
      "placeholder": "2.0",
      "defaultValue": "2",
      "description": "dose in Gy",
      "validations": {
        "type": "float",
        "min": "0.000001",
        "max": "10000000000000000"
      }
    },
    {
      "type": "select",
      "parameterName": "material_no",
      "label": "Material",
      "values": "materials",
      "asManyAsPoints": false,
      "defaultValue": 1,
      "description": "Choose material type"
    }
  ],
  "moreOptions": {
    "visible": true,
    "defaultXAxisType": "linear",
    "defaultYAxisType": "linear",
    "plotType": "lines"
  },
  "modals": {
    "dataSeries": true,
    "download": true,
    "showDataSeries": true,
    "deleteAll": true
  }
}
